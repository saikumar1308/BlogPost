
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String   @id @default(uuid())
  username String @unique
  name String?
  password String
  blogs Blog[]
}

model Blog {
  id String   @id @default(uuid())
  authorId String
  content String
  title String
  published Boolean @default(false)
  author  User @relation(fields: [authorId], references: [id])
}
